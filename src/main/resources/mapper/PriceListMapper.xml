<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rain.dao.mapper.ExcelDataMapper">
    <resultMap id="BaseResultMap" type="com.rain.dao.entity.User">
        <result column="SUPPLIER" property="supplier" jdbcType="VARCHAR" />
        <result column="PART_NUMBER" property="part_number" jdbcType="VARCHAR" />
        <result column="ORIG_PART_NUMBER" property="orig_part_number" jdbcType="VARCHAR" />
        <result column="DESCRIPTION" property="description" jdbcType="VARCHAR" />
        <result column="KEY_WORD" property="key_word" jdbcType="VARCHAR" />
        <result column="LIST_PRICE" property="list_price" jdbcType="VARCHAR" />
        <result column="NET_PRICE" property="net_price" jdbcType="VARCHAR" />
        <result column="UNIT_PRICE" property="unit_price" jdbcType="VARCHAR" />
        <result column="PRICE" property="price" jdbcType="VARCHAR" />
        <result column="LEAD_TIME" property="lead_time" jdbcType="VARCHAR" />
        <result column="CAGE_CODE" property="cage_code" jdbcType="VARCHAR" />
        <result column="AIRCRAFT" property="aircraft" jdbcType="VARCHAR" />
        <result column="SELLING_UOM_CD" property="selling_uom_cd" jdbcType="VARCHAR" />
        <result column="QTY_PER" property="qty_per" jdbcType="VARCHAR" />
        <result column="STDPKGQTY" property="stdpkgqty" jdbcType="VARCHAR" />
        <result column="MOQ" property="moq" jdbcType="VARCHAR" />
    </resultMap>

    <insert id="insertPriceCata" parameterType="java.util.List">
        insert into aog_price_catalugue_2007 (ID, SUPPLIER, PART_NUMBER,
        ORIG_PART_NUMBER, DESCRIPTION, KEY_WORD,LIST_PRICE, NET_PRICE,
        UNIT_PRICE,PRICE,LEAD_TIME,CAGE_CODE,AIRCRAFT,SELLING_UOM_CD,QTY_PER,
        STDPKGQTY,MOQ)
        VALUE
        <foreach collection="list" item="item" index="index" separator="," >
            (#{item.supplier},#{item.partNumber},#{item.OrigPartNumber},
            #{item.description},#{item.keyWord},#{item.listPrice},#{item.netPrice},
            #{item.unitPrice},#{item.price},#{item.leadTime},#{item.cageCode},
            #{item.aircraft},#{item.SellingUomCd},#{item.qtyPer},#{item.stdpkgqty},
            #{item.moq})
        </foreach>
    </insert>
</mapper>